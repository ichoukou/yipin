<struts>
	<package name="notice" extends="yipin-default" namespace="/admin/notice">
		<action name="notice-*"
			class="com.ytoxl.yipin.web.action.notice.NoticeAction" method="{1}">
			<result name="searchNotice">/WEB-INF/pages/notice/searchNotice.jsp</result>
			<result name="editNoitce">/WEB-INF/pages/notice/editNotice.jsp</result>
			<result name="viewNoitce">/WEB-INF/pages/notice/viewNotice.jsp</result>
			<result name="turnAction" type="redirectAction">${nextAction}</result>
		</action>
	</package>
</struts>