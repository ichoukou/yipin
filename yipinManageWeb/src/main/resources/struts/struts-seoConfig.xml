<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="seo" extends="yipin-default" namespace="/admin/seo">
    	<action name="seo-*" class="com.ytoxl.yipin.web.action.content.SeoConfigAction" 
    		method="{1}">
    	   <result name="listSeoConfigs">/WEB-INF/pages/seo/searchSeoConfig.jsp</result>
    	   <result name="saveSeoConfig" type="redirectAction">${nextAction}</result>
    	   <result type="json">
    			<param name="includeProperties">
    				seoConfig.*
    			</param>
    		</result>
    	</action>
    	<action name="repeatUrlKey" class="com.ytoxl.yipin.web.action.content.SeoConfigAction" 
    		method="repeatUrlKey">
    		<result type="json">
    			<param name="includeProperties">
    				seoConfig.*
    			</param>
    		</result>
    		</action>
	</package>
</struts>